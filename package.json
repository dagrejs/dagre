{
  "name": "@dagrejs/dagre",
  "version": "3.0.0-pre",
  "description": "Graph layout for JavaScript",
  "author": "Chris Pettitt <cpettitt@gmail.com>",
  "contributors": [
    "David Newell",
    "Matthew Dahl (https://github.com/sandersky)"
  ],
  "license": "MIT",
  "main": "dist/dagre.cjs.js",
  "module": "dist/dagre.esm.js",
  "scripts": {
    "build": "npm run version:make && npm run build:types && tsx build.ts",
    "build:types": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "version:make": "tsx src/release/make-version.ts > lib/version.ts",
    "version:check": "tsx src/release/check-version.ts",
    "version:bump": "tsx src/release/bump-version.ts",
    "bench": "tsx src/bench.ts",
    "prepublishOnly": "npm run typecheck && npm run lint && npm test && npm run build"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/dagre.esm.js",
      "require": "./dist/dagre.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/types/index.d.ts",
  "keywords": [
    "graph",
    "layout"
  ],
  "dependencies": {
    "@dagrejs/graphlib": "3.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/benchmark": "^2.1.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "benchmark": "^2.1.4",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.1",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "semver": "^7.7.4",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dagrejs/dagre.git"
  }
}
